{
    "name": "redeman/imgur-laravel",
    "description": "Laravel Package for Imgur API. Provides easy access to the adyg/php-imgur-api-client library.",
    "keywords": ["Laravel", "imgur", "api", "wrapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Redeman",
            "email": "markredeman@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7|^8",
        "laravel/framework": "^5.8|^6|^7|^8",
        "j0k3r/php-imgur-api-client": "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Redeman\\Imgur\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redeman\\Imgur\\ImgurServiceProvider"
            ]
        }
    }
}
